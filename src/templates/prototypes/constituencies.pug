extends ../../layouts/layout.pug
block content
	.section--primary(id="content")
		.container
			.section--primary__container
				.section--primary__block
					//- Page Title
					h1 Current and former constituencies

			.section--primary__container
				.section--primary__block
					p A to Z - Showing results for A
					//- Letter Navigation
					include ../../components/navigation-letter.pug

	section
		.container
			//- Result List
			ol.list--box
				each object in [{name: 'Aberdeen North', state: 'Former constituency', date: 'From 9 Jun 1983 to 1 May 1997'},{name: 'Aberdeen North', state: 'Current constituency', date: 'From 5 Apr 2005 to present'},{name: 'Aberdeen North', state: 'Former constituency', date: 'From 14 Dec 1918 to 23 Feb 1950'}]
					li
						h2
							a(href='#')
								= object.name
								span.sr-only
									= object.state + ', ' + object.date
